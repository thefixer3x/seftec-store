{
  "name": "seftec-store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/seftec-store/src",
  "projectType": "application",
  "tags": ["type:ui", "scope:ecommerce", "framework:vite", "status:live", "domain:seftechub.com"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/seftec-store"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "seftec-store:build",
        "port": 9994,
        "host": "0.0.0.0"
      },
      "configurations": {
        "development": {
          "buildTarget": "seftec-store:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "seftec-store:build:production",
          "hmr": false
        }
      }
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "seftec-store:build:development",
        "port": 9994,
        "host": "0.0.0.0",
        "hmr": true
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "seftec-store:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/seftec-store/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "passWithNoTests": true
      }
    }
  }
}